            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="departmentField" layoutX="73.0" layoutY="365.0" prefHeight="50.0" prefWidth="340.0" promptText="   Department" styleClass="txtFields" stylesheets="@LoginStyle.css">
