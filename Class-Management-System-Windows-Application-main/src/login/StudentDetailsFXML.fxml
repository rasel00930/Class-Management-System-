      </Label>
      <TextField fx:id="firstNameField" layoutX="73.0" layoutY="110.0" prefHeight="50.0" prefWidth="340.0" promptText="   First Name" styleClass="txtFields" stylesheets="@LoginStyle.css">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="lastNameField" layoutX="73.0" layoutY="195.0" prefHeight="50.0" prefWidth="340.0" promptText="   Last Name" styleClass="txtFields" stylesheets="@LoginStyle.css">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="institutionField" layoutX="73.0" layoutY="280.0" prefHeight="50.0" prefWidth="340.0" promptText="   Institution" styleClass="txtFields" stylesheets="@LoginStyle.css">
